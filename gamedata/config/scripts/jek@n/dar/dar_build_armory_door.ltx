[logic]
active = ph_door@close

[ph_door@close]
locked = false
closed = true
show_tips = true
tip_open = dar_build_armory_door_use
on_use = {=actor_has_item(dar_build_oficer_key)} ph_door@open %=remove_item(dar_build_oficer_key)%


[ph_door@open]
locked = false
closed = false
on_use = ph_door@free


[ph_door@free]
locked = false
closed = true
no_force = true
on_use = ph_door@open