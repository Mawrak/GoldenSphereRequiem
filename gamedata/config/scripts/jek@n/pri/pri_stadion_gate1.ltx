[collide]
ignore_static

[ph_heavy]

[logic]
active = ph_gate@locked

[ph_gate@locked]
state = closed
locking = hard
breakable = false
on_use = {=actor_has_item(pri_stadion_key)} ph_gate@open %=remove_item(pri_stadion_key)%

[ph_gate@unlocked]
state = closed
locking = stick
breakable = false

[ph_gate@open]
state = open
locking = stick
breakable = false
left_limit = 80
right_limit = 80
;on_info = {+pri_stadion_agent_info} ph_gate@locked