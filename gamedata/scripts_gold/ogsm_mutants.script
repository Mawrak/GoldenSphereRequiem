function cat_sleep(p_obj)
rnd = math.random(1, 10)
if rnd == 1 then 
	level.add_pp_effector("agr_u_fade.ppe", 2007, false)
	level.add_pp_effector("total_recall.ppe", 2008, false)
	db.actor:stop_talk()
	db.actor.power =-0.05
end
if rnd == 2 then 
	--level.add_pp_effector("agr_u_fade.ppe", 2007, false)
	--level.add_pp_effector("total_recall.ppe", 2008, false)
	--db.actor:stop_talk()
	--db.actor.power =-0.05
end
