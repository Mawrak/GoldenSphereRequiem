	--level.add_pp_effector("agr_u_fade.ppe", 2007, false)
	--level.add_pp_effector("total_recall.ppe", 2008, false)
	--db.actor:stop_talk()
	--db.actor.power =-0.05
	--soundtrack.actor_need_sleep()
