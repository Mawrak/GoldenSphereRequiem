function cat_sleep(p_obj)
rnd = math.random(1, 10)
if rnd == 1 then 
	level.add_pp_effector("agr_u_fade.ppe", 2007, false)
	level.add_pp_effector("total_recall.ppe", 2008, false)
	db.actor:stop_talk()
	db.actor.power =-0.05
end
if rnd == 2 then 
	--level.add_pp_effector("agr_u_fade.ppe", 2007, false)
	--level.add_pp_effector("total_recall.ppe", 2008, false)
	--db.actor:stop_talk()
	--db.actor.power =-0.05
end
if rnd == 3 then 
	--level.add_pp_effector("agr_u_fade.ppe", 2007, false)
	--level.add_pp_effector("total_recall.ppe", 2008, false)
	--db.actor:stop_talk()
	--db.actor.power =-0.05
	--soundtrack.actor_need_sleep()
end
if rnd == 3 then 
soundtrack.psy_cat_sounds()
end
if rnd == 5 then 
--soundtrack.psy_cat_sounds()
end
if rnd == 6 then 
soundtrack.psy_cat_sounds()
end
if rnd == 7 then 
--soundtrack.psy_cat_sounds()
end
if rnd == 8 then 
soundtrack.psy_cat_sounds()
end
if rnd == 9 then 
--soundtrack.psy_cat_sounds()
end
if rnd == 10 then 
soundtrack.psy_cat_sounds()
end
end



