	if chimera_sound and not chimera_sound:playing() then 
		chimera_sound:play_at_pos(p_obj, p_obj:position(), 0)
		local zh = hit()
		zh.draftsman = enemy
		zh.type = hit.shock
		zh.direction = p_obj:position():sub(enemy:position())
		zh:bone("pelvis")
		zh.power = 0.2
		zh.impulse = 0.1
		enemy:hit(zh)
	end
end
end


